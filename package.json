{
  "name": "moon-react-boilerplate",
  "version": "1.0.0",
  "description": "Moon Very Own React Boilerplate Code",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "clean": "rimraf dist node_modules",
    "test": "jest --config jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nerdmj/moon-boilerplate.git"
  },
  "keywords": [
    "Boilerplate",
    "React",
    "nerdMj"
  ],
  "author": "mnsh <er.manishjatwani@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nerdmj/moon-boilerplate/issues"
  },
  "homepage": "https://github.com/nerdmj/moon-boilerplate#readme",
  "dependencies": {
    "babel-jest": "^22.4.3",
    "enzyme-to-json": "^3.3.3",
    "express": "^4.16.3",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "webpack": "^4.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "react-test-renderer": "^16.3.2",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
